<h1>创建用户</h1>
<%= form_for :user do |f| %>
    <p>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </p>
    <p>
        <%= f.label :phone %>
        <%= f.telephone_field :phone %>
    </p>
    <p>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </p>
    <p>
        <%= f.submit %>
    </p>
<% end %>
