<h1>登录</h1>
<%= form_with(model: @user, url: login_path, local: true )do |form| %>
    <p>
        <%= form.label :phone %>
        <%= form.telephone_field :phone%>
    </p>
    <p>
        <%= form.label :password %>
        <%= form.password_field :password %>
    </p>
    <p><%= form.submit %></p>
<% end %>